{
  "presets": [
    "razzle/babel"
  ],
  "plugins": [
    [
      "react-css-modules",
      {
        "filetypes": {
          ".scss": {
            "syntax": "postcss-scss",
            "plugins": ["postcss-nested"]
          }
        },
        "generateScopedName": "[name]__[local]___[hash:base64:5]"
      }
    ],
    "react-loadable/babel",
    "macros"
  ]
}
